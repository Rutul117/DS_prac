source ~/.zshrc
/opt/spark/bin/spark-shell 


class Person(name: String, age: Int) {
  def greet(): Unit = {
    println(s"Hello, my name is $name and I am $age years old.")
  }
}

object Main {
  def main(args: Array[String]): Unit = {
    val person = new Person("John", 30)
    person.greet()
  }
}
